{
    "name": "vega-specs",
    "version": "1.0.0",
    "description": "wrestling with Vega3",
    "scripts": {
        "watch": "watchify ./6/app.js -t [ babelify ] -o ./6/build/app.js --debug --verbose",
        "server": "python -m SimpleHTTPServer 9001",
        "start": "npm run server & npm run watch",
        "build_2": "browserify ./2/app.js -t [ babelify ] -o ./2/build/app.js",
        "build_3": "browserify ./3/app.js -t [ babelify ] -o ./3/build/app.js",
        "build_4": "browserify ./4/app.js -t [ babelify ] -o ./4/build/app.js",
        "build_5": "browserify ./5/app.js -t [ babelify ] -o ./5/build/app.js",
        "build_6": "browserify ./6/app.js -t [ babelify ] -o ./6/build/app.js",
        "build": "npm run build_2 && npm run build_3 && npm run build_4 && npm run build_5 && npm run build_6"
    },
    "author": "abudaan",
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "6.24.0",
        "babel-eslint": "^7.2.3",
        "babel-preset-es2015": "6.13.2",
        "babel-preset-stage-0": "6.5.0",
        "babelify": "^7.3.0",
        "browserify": "^14.4.0",
        "core-js": "^2.4.1",
        "eslint": "3.19.0",
        "eslint-config-airbnb": "14.1.0",
        "eslint-plugin-babel": "4.1.1",
        "eslint-plugin-flowtype": "2.30.4",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-jsx-a11y": "4.0.0",
        "eslint-plugin-react": "6.10.0",
        "flow-bin": "0.42.0",
        "watchify": "^3.9.0"
    },
    "dependencies": {
        "leaflet": "^1.1.0",
        "ramda": "0.23.0",
        "vega": "^3.0.0-rc5",
        "vega-tooltip": "^0.4.2",
        "yamljs": "^0.2.10"
    }
}
